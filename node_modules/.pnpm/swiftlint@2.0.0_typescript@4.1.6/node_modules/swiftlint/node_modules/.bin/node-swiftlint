#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sig-service/Desktop/sig-projects/plugins/capacitor-apk-installer/node_modules/.pnpm/swiftlint@2.0.0_typescript@4.1.6/node_modules/swiftlint/node_modules:/Users/sig-service/Desktop/sig-projects/plugins/capacitor-apk-installer/node_modules/.pnpm/swiftlint@2.0.0_typescript@4.1.6/node_modules:/Users/sig-service/Desktop/sig-projects/plugins/capacitor-apk-installer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sig-service/Desktop/sig-projects/plugins/capacitor-apk-installer/node_modules/.pnpm/swiftlint@2.0.0_typescript@4.1.6/node_modules/swiftlint/node_modules:/Users/sig-service/Desktop/sig-projects/plugins/capacitor-apk-installer/node_modules/.pnpm/swiftlint@2.0.0_typescript@4.1.6/node_modules:/Users/sig-service/Desktop/sig-projects/plugins/capacitor-apk-installer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
